<!DOCTYPE html>
<html>
  <head>
    <title>Work Shop</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>

  </head>

  <body>


    <div class = "container">
    <%= render 'layouts/navigation' %>
    <% flash.each do |name, msg| %>
      <%= msg %>
    <% end %>
    <%= yield %>
    </div>
  </body>
</html>
